<%- include('header'); -%>
<body>
<div class="box" id="heading">
    <h1> <%= ListTitle %> </h1>
</div>    

<div class="box">
    <% for (var i=0; i<newListItem.length; i++ ){ %>
    <div class="item">
        <input type="checkbox">
             <p><%= newListItem[i] %></p> 
    </div>
    <% } %>

    <form action="/" method="post" class="item">
        <input id="newList" type="text" name="newList" placeholder="new item" autocomplete="off">
        <button type="submit" name="button" value= <%= ListTitle %>>+</button>
    </form>

    <!-- <a href="/Work">GO TO WORK SECTION</a> -->
</div>


</body>
<%- include('footer'); -%>




